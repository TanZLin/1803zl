			$("#qr_container").show();
			var sku_goods=prodInfo.cNo;
			var text = 'http://m.yougou.com/c-null/sku-null-'+prodInfo.cNo;
			if($("#div_div canvas").length==0){
				$('#qr_container').qrcode({
					render: 'canvas',
					text: utf16to8(text),
					height: 120,
					width: 120,
					//logo图片地址
					src: 'http://m.yougou.com/images/apple_icon/144.png'
				});
			}else{
				$("#div_div canvas").show();
			}
		},function(){
			$("canvas").hide();
			$("#qr_container").hide();
		});
		function utf16to8(str) { //转码
			var out, i, len, c;
			out = "";
			len = str.length;
			for (i = 0; i < len; i++) {
				c = str.charCodeAt(i);
				if ((c >= 0x0001) && (c <= 0x007F)) {
					out += str.charAt(i);
				} else if (c > 0x07FF) {
					out += String.fromCharCode(0xE0 | ((c >> 12) & 0x0F));
					out += String.fromCharCode(0x80 | ((c >> 6) & 0x3F));
					out += String.fromCharCode(0x80 | ((c >> 0) & 0x3F));
				} else {
					out += String.fromCharCode(0xC0 | ((c >> 6) & 0x1F));
					out += String.fromCharCode(0x80 | ((c >> 0) & 0x3F));
				}
			}
			return out;
		}
	})
</script>
